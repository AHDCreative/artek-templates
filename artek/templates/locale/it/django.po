#: templates/account/password_reset_token_fail.html:5
#: templates/account/password_reset_token_fail.html:8
msgid "Bad token"
msgstr "Token non valido"

#: templates/account/password_reset_token_fail.html:10
msgid "The password reset link was invalid, possibly because it has already been used.  Please request a <a href=\"%(url)s\">new password reset</a>."
msgstr "Il link di reimpostazione della password non era valido, probabilmente perché è già stato utilizzato. Richiedi un <a href=\"%(url)s\"> reimpostazione nuova password </a>."

